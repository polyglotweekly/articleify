{
  "name": "articleify",
  "version": "1.0.1",
  "description": "a tool for generating static sites from directories full of markdown articles",
  "main": "./lib/index.js",
  "bin": "./bin/articleify.js",
  "scripts": {
    "test": "mocha --check-leaks --ui exports --require blanket -R mocoverage"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "output-reporter": "spec"
    }
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/polyglotweekly/articleify.git"
  },
  "keywords": [
    "markdown",
    "jekyll",
    "blog",
    "blogging",
    "articles"
  ],
  "author": "Ben Coe <ben@npmjs.com>",
  "license": "ISC",
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^2.1.0",
    "cheerio": "^0.18.0",
    "mocha": "^2.1.0",
    "mocoverage": "^1.0.0",
    "recursive-readdir": "^1.2.1",
    "rimraf": "^2.2.8"
  },
  "dependencies": {
    "bluebird": "^2.9.13",
    "chalk": "^1.0.0",
    "handlebars": "^3.0.0",
    "inquirer": "^0.8.0",
    "lodash": "^3.3.1",
    "marky-markdown": "^5.0.2",
    "mkdirp": "^0.5.0",
    "moment": "^2.9.0",
    "pretty-filename": "^0.1.0",
    "yargs": "^3.4.5"
  }
}
